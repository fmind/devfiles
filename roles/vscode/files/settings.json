{
    "editor.acceptSuggestionOnEnter": "off",
    "editor.fontFamily": "'Fira Code', monospace",
    "editor.fontLigatures": true,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.intellisense.unimathsymbols.enabled": true,
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "python.dataScience.changeDirOnImportExport": false,
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "python.linting.mypyEnabled": true,
    "python.unitTest.pyTestEnabled": true,
    "vim.foldfix": true,
    "vim.leader": "<space>",
    "vim.sneak": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindings": [
        {"before": ["<enter>"], "after": [":"]},
        {"before": ["<leader>", "a"], "commands": ["workbench.action.navigateBack"]},
        {"before": ["<leader>", "b"], "commands": ["workbench.action.debug.start"]},
        {"before": ["<leader>", "c"], "commands": ["editor.action.showContextMenu"]},
        {"before": ["<leader>", "d"], "commands": ["editor.debug.action.toggleBreakpoint"]},
        {"before": ["<leader>", "e"], "commands": ["workbench.action.nextEditor"]},
        {"before": ["<leader>", "f"], "commands": ["actions.find"]},
        {"before": ["<leader>", "g"], "commands": ["workbench.action.gotoLine"]},
        {"before": ["<leader>", "h"], "commands": ["editor.action.triggerParameterHints"]},
        {"before": ["<leader>", "i"], "commands": ["editor.action.insertLineAfter"]},
        {"before": ["<leader>", "j"], "commands": ["workbench.action.gotoSymbol"]},
        {"before": ["<leader>", "k"], "commands": ["editor.action.quickFix"]},
        {"before": ["<leader>", "l"], "commands": ["editor.action.revealDefinition"]},
        {"before": ["<leader>", "m"], "commands": ["workbench.action.editor.changeLanguageMode"]},
        {"before": ["<leader>", "n"], "commands": ["editor.action.nextMatchFindAction"]},
        {"before": ["<leader>", "o"], "commands": ["workbench.action.quickOpen"]},
        {"before": ["<leader>", "p"], "commands": ["workbench.action.showCommands"]},
        {"before": ["<leader>", "q"], "commands": ["workbench.action.closeActiveEditor"]},
        {"before": ["<leader>", "r"], "commands": ["editor.action.rename"]},
        {"before": ["<leader>", "s"], "commands": ["editor.action.triggerSuggest"]},
        {"before": ["<leader>", "t"], "commands": ["workbench.action.tasks.test"]},
        {"before": ["<leader>", "u"], "commands": ["editor.action.referenceSearch.trigger"]},
        {"before": ["<leader>", "v"], "commands": ["workbench.action.quickOpenView"]},
        {"before": ["<leader>", "w"], "commands": ["editor.action.toggleWordWrap"]},
        {"before": ["<leader>", "x"], "commands": ["workbench.extensions.action.installExtension"]},
        {"before": ["<leader>", "y"], "commands": [""]},
        {"before": ["<leader>", "z"], "commands": ["editor.fold"]},
        {"before": ["<leader>", "A"], "commands": ["workbench.action.navigateForward"]},
        {"before": ["<leader>", "B"], "commands": ["workbench.view.debug"]},
        {"before": ["<leader>", "C"], "commands": [""]},
        {"before": ["<leader>", "D"], "commands": [""]},
        {"before": ["<leader>", "E"], "commands": ["workbench.action.previousEditor"]},
        {"before": ["<leader>", "F"], "commands": ["workbench.action.replaceInFiles"]},
        {"before": ["<leader>", "G"], "commands": ["workbench.view.scm"]},
        {"before": ["<leader>", "H"], "commands": [""]},
        {"before": ["<leader>", "I"], "commands": ["editor.action.insertLineBefore"]},
        {"before": ["<leader>", "J"], "commands": ["workbench.action.showAllSymbols"]},
        {"before": ["<leader>", "K"], "commands": ["workbench.action.openGlobalKeybindings"]},
        {"before": ["<leader>", "L"], "commands": [""]},
        {"before": ["<leader>", "M"], "commands": ["workbench.actions.view.problems"]},
        {"before": ["<leader>", "N"], "commands": ["editor.action.previousMatchFindAction"]},
        {"before": ["<leader>", "O"], "commands": ["workbench.action.output.toggleOutput"]},
        {"before": ["<leader>", "P"], "commands": ["workbench.action.files.copyPathOfActiveFile"]},
        {"before": ["<leader>", "Q"], "commands": ["workbench.action.closeOtherEditors"]},
        {"before": ["<leader>", "R"], "commands": [""]},
        {"before": ["<leader>", "S"], "commands": [""]},
        {"before": ["<leader>", "T"], "commands": ["workbench.action.selectTheme"]},
        {"before": ["<leader>", "U"], "commands": [""]},
        {"before": ["<leader>", "V"], "commands": ["workbench.action.toggleSidebarVisibility"]},
        {"before": ["<leader>", "W"], "commands": [""]},
        {"before": ["<leader>", "X"], "commands": ["workbench.view.extensions"]},
        {"before": ["<leader>", "Y"], "commands": ["workbench.action.openSnippets"]},
        {"before": ["<leader>", "Z"], "commands": ["editor.unfold"]},
        {"before": ["<leader>", "`"], "commands": ["workbench.view.explorer"]},
        {"before": ["<leader>", "-"], "commands": ["workbench.action.splitEditor"]},
        {"before": ["<leader>", "="], "commands": ["editor.action.formatDocument"]},
        {"before": ["<leader>", "["], "commands": ["editor.action.marker.prevInFiles"]},
        {"before": ["<leader>", "]"], "commands": ["editor.action.marker.nextInFiles"]},
        {"before": ["<leader>", ";"], "commands": ["editor.action.commentLine"]},
        {"before": ["<leader>", "'"], "commands": ["workbench.action.terminal.toggleTerminal"]},
        {"before": ["<leader>", ","], "commands": ["workbench.action.openWorkspaceSettings"]},
        {"before": ["<leader>", "."], "commands": ["workbench.action.openSettings"]},
        {"before": ["<leader>", "/"], "commands": ["workbench.view.search"]},
        {"before": ["<leader>", "<tab>"], "commands": ["workbench.action.openPreviousRecentlyUsedEditorInGroup"]},
        {"before": ["<leader>", "<space>"], "commands": ["workbench.action.tasks.build"]}
    ],
    "vim.visualModeKeyBindings": [
        {"before": [ "<enter>" ], "after": [ ":" ]}
    ],
    "workbench.colorTheme": "Default Light+"
}
